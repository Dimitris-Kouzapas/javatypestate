package buyer_seller;

typestate Buyer1Protocol {
	session
		InitSession = 	{
							void sendTitleToSeller(String title): TitleSent
						}
	where
		TitleSent =		{
							int receivePriceFromSeller(): PriceReceived
						}

		PriceReceived = {
							void sendQuoteToBuyer2(int quote): QuoteSent
						}
		
		QuoteSent = 	{
							AgreementLabel receiveLabelFromBuyer2(): <AGREE: TalktoSeller, QUIT: end>
						}

		TalktoSeller =	{
							void transferMoneyToSeller(int money): end
						}
}