package buyer_seller;

typestate SellerProtocol {
	session
		InitSession =		{
								void receiveTitleFromBuyer1(String): TitleReceived
							}
	where
		TitleReceived =		{
								String sendPriceToBuyer1(): PriceSent
							}

		PriceSent =			{
								AgreementLabel receiveLabelFromBuyer2(): <AGREE: TalktoBothBuyers, QUIT: end>
							}

		TalktoBothBuyers =	{
								void receiveMoneyFromBothByers(String): end
							}	
}