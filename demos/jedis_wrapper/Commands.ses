package demos.jedis_wrapper;

typestate Commands {
	session
		InitSession = {
           void watch(String[]): Watching
		}
	where
	    // Use commas rather than semicolons to separate members
		Watching = {
			String get(String): Watching,
			List lrange(): Watching,
			void watch(String[]): Watching,
			void multi(): Queued
		}

		Queued = {
		    void set(String, String): Queued,
		    String lpop (String): Queued,
			void discard(): end,
			Result exec(): <OK:end, FAIL:Queued>
		}
}
