package demos.redis.jediswrapper;

typestate Redis_ServerAsync {
	session
		Init = {
           void WATCH(String[]): Watching
		}
	where
		Watching = {
			void GET_request(String): Watching,
			void WATCH(String[]): Watching,
			void MULTI(): Queued
		}
		
		Queued = {
		    void SET(String, String): Queued,
			void DISCARD(): end,
			void EXEC(): Watching
		}
}
