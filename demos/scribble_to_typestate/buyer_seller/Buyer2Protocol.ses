package demos.scribble_to_typestate.buyer_seller;

typestate Buyer2Protocol {
	session
		State0 = {
			Int receivequoteIntfromBuyer1(): State1
		}
	where
		State1 = {
			ChoiceOption sendLabeltoBuyer1Seller(String):
			<AGREE: State10, QUIT:State11>
		}

		State10 = {
			void sendPayloadforAGREEtoBuyer1Seller(String): State2
		}

		State11 = {
			void sendPayloadforQUITtoBuyer1Seller(String): end
		}

		State2 = {
			void sendtransferInttoSeller(Int):end
		}
}