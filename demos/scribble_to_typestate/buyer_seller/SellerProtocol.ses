package demos.scribble_to_typestate.buyer_seller;

typestate SellerProtocol {
	session
		State0 = {
			String receivebookStringfromBuyer1(): State1
		}
	where
		State1 = {
			void sendbookInttoBuyer1(Int): State2
		}

		State2 = {
			ChoiceOption receiveLabelfromBuyer2():
			<AGREE:State20, QUIT:State21>
		}

		State20 = {
			String receivePayloadforAGREEfromBuyer2(): State3
		}

		State21 = {
			String receivePayloadforQUITfromBuyer2(): end
		}

		State3 = {
			Int receivetransferIntfromBuyer1(): State4
		}

		State4 = {
			Int receivetransferIntfromBuyer2(): end
		}
}