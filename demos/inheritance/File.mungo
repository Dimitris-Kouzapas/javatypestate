package demos.inheritance;

public class File typestate FileProtocol {
    private FileHandle h;

    public File () {
        h = new FileHandle();
    }

    public void open () {
        h.acquire();
        byte header = read(); // let's say every file starts with a header byte
    }

    public byte read () {
        return 0;
    }

    public void close () {
        h.release();
    }
}
