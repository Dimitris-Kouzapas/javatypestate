package file_example;
typestate FileProtocol {

	session
		Init =		{
					Status open(): <OK: Open, ERROR: Init>
				}
	where
    
		Open =		{
					BooleanEnum hasNext(): <TRUE: Read, FALSE: Close>,
					void close(): Init
				}

		Read =		{
					void read(): Open
				}
	    
		Close =		{
					void close(): Init 
				}
}
